			"name": "employ data",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let json={};\r",
							"try{\r",
							"    json=pm.response.json();\r",
							"}\r",
							"catch(e)\r",
							"{\r",
							"    console.log(e);\r",
							"}\r",
							"pm.test(\"status code\",()=>{\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"pm.test(\"Content-Type header is application/json\", () => {\r",
							"    pm.expect(pm.response.headers.get(\"content-type\")).to.include(\"application/json\");\r",
							"});\r",
							"pm.test(\"response time\", () => {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(900);\r",
							"});\r",
							"pm.test(\"status msg\", () => {\r",
							"    pm.expect(json).to.have.property(\"status\");\r",
							"    pm.expect(json.status).to.be.a(\"string\").and.to.not.be.empty;\r",
							"});\r",
							"pm.test(\"message\", () => {\r",
							"    pm.expect(json.message).to.be.a(\"string\").and.to.not.be.empty;\r",
							"    pm.expect(json.message.toLowerCase()).to.include(\"successfully\");\r",
							"});\r",
							"pm.test(\"data is array\", () => {\r",
							"    pm.expect(json).to.have.property(\"data\");\r",
							"    pm.expect(json.data).to.be.an(\"array\");\r",
							"    \r",
							"});\r",
							"\r",
							"pm.test(\"String fields validation\", () => {\r",
							"    const stringFields = [\"employee_name\",\"employee_salary\",\"employee_age\" ];\r",
							"\r",
							"    json.data.forEach((employee) => {\r",
							"        pm.expect(employee).to.have.property(\"id\");\r",
							"        pm.expect(employee.id).to.be.a(\"number\");\r",
							"    \r",
							"    stringFields.forEach(field => {\r",
							"        pm.expect(employee[field]).to.be.a(\"string\").and.to.not.be.empty;  \r",
							"    });\r",
							"    });\r",
							"});\r",
							"pm.test(\"Profile image is a string (can be empty)\", () => {\r",
							"    json.data.forEach(employee => {\r",
							"        pm.expect(employee.profile_image).to.be.a(\"string\");\r",
							"        // optional: check length >= 0 (allows empty string)\r",
							"        pm.expect(employee.profile_image.length >= 0).to.be.true;\r",
							"    });\r",
							"});"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
