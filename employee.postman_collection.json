		{
			"name": "single employ data",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"let json = {};\r",
							"try {\r",
							"    json = pm.response.json();\r",
							"} catch (e) {\r",
							"    console.log(e);\r",
							"}\r",
							"\r",
							"// Status code test\r",
							"pm.test(\"Status code is 200\", () => {\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"\r",
							"// Response time test\r",
							"pm.test(\"Response time is below 900ms\", () => {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(900);\r",
							"});\r",
							"\r",
							"// Header test\r",
							"pm.test(\"Content-Type header is JSON\", () => {\r",
							"    pm.expect(pm.response.headers.get(\"content-type\")).to.include(\"application/json\");\r",
							"});\r",
							"\r",
							"// Status field test\r",
							"pm.test(\"Status field validation\", () => {\r",
							"    pm.expect(json).to.have.property(\"status\");\r",
							"    pm.expect(json.status).to.be.a(\"string\");\r",
							"});\r",
							"\r",
							"// Message field test\r",
							"pm.test(\"Message field validation\", () => {\r",
							"    pm.expect(json).to.have.property(\"message\").and.to.be.a(\"string\").and.not.to.be.empty;\r",
							"    pm.expect(json.message.toLowerCase()).to.include(\"successfully\");\r",
							"});\r",
							"\r",
							"// Data field test\r",
							"pm.test(\"Data field validation\", () => {\r",
							"    const data = json.data;\r",
							"\r",
							"    if (data === null) {\r",
							"        console.log(\"No employee data available.\");\r",
							"        pm.expect(data).to.be.null; // Optional check\r",
							"        return; // Skip further validations\r",
							"    }\r",
							"\r",
							"    // Validate employee object\r",
							"    pm.expect(data).to.be.an(\"object\").and.not.to.be.empty;\r",
							"    pm.expect(data).to.have.property(\"id\").and.satisfy(id => !isNaN(Number(id)));\r",
							"    pm.expect(data).to.have.property(\"employee_name\").and.to.be.a(\"string\").and.not.to.be.empty;\r",
							"    pm.expect(data).to.have.property(\"employee_salary\").and.to.be.a(\"string\").and.not.to.be.empty;\r",
							"    pm.expect(data).to.have.property(\"employee_age\").and.to.be.a(\"string\").and.not.to.be.empty;\r",
							"});\r",
							"\r",
							"// Profile image test\r",
							"pm.test(\"Profile image is string or null\", () => {\r",
							"    const data = json.data;\r",
							"    if (data === null) {\r",
							"        pm.expect(data).to.be.null; // Optional\r",
							"        return;\r",
							"    }\r",
							"    const profileImage = data.profile_image;\r",
							"    pm.expect(profileImage === null || typeof profileImage === \"string\").to.be.true;\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
