		{
			"name": "create booking",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "8a7637fc-843b-4649-a053-56e1eb6a6fac",
						"exec": [
							"// Generate random number\r",
							"const randomNum = Math.floor(Math.random() * 10000);\r",
							"\r",
							"// Generate firstname & lastname using pattern like user1234 and test1234\r",
							"pm.environment.set(\"firstName\", `user${randomNum}`);\r",
							"pm.environment.set(\"lastName\", `test${randomNum}`);\r",
							"\r",
							"\r",
							"// Log output\r",
							"console.log(\"Generated First Name:\", pm.environment.get(\"firstName\"));\r",
							"console.log(\"Generated Last Name:\", pm.environment.get(\"lastName\"));\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "e518a7ad-2ec7-4e80-accb-e740d964abb7",
						"exec": [
							"let json = {};\r",
							"try {\r",
							"    json = pm.response.json();\r",
							"} catch (e) {\r",
							"    console.log(e);\r",
							"}\r",
							"\r",
							"// ⭐ Save booking ID if available\r",
							"if (json.bookingid) {\r",
							"    pm.environment.set(\"bookingid\", json.bookingid);\r",
							"    console.log(\"Booking ID saved to environment:\", json.bookingid);\r",
							"} else {\r",
							"    console.warn(\"⚠ No bookingid found in response\");\r",
							"}\r",
							"\r",
							"// -------- TESTS --------\r",
							"\r",
							"pm.test(\"Status check\", () => {\r",
							"    pm.expect(pm.response.code).to.oneOf([200,201]);\r",
							"});\r",
							"\r",
							"pm.test(\"Content header\", () => {\r",
							"    pm.expect(pm.response.headers.get(\"content-type\")).to.include(\"application/json\");\r",
							"});\r",
							"\r",
							"pm.test(\"Response time\", () => {\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							"\r",
							"pm.test(\"Response body validation\", () => {\r",
							"    pm.expect(json).to.have.property(\"bookingid\").and.to.be.a(\"number\");\r",
							"    pm.expect(json).to.have.property(\"booking\").and.to.be.an(\"object\");\r",
							"    pm.expect(json.booking).to.have.property(\"firstname\").and.to.be.a(\"string\");\r",
							"    pm.expect(json.booking).to.have.property(\"lastname\").and.to.be.a(\"string\");\r",
							"    pm.expect(json.booking).to.have.property(\"totalprice\").and.to.be.a(\"number\");\r",
							"    pm.expect(json.booking).to.have.property(\"depositpaid\").and.to.be.a(\"boolean\");\r",
							"    pm.expect(json.booking).to.have.property(\"bookingdates\").and.to.be.an(\"object\");\r",
							"    pm.expect(json.booking.bookingdates).to.have.property(\"checkin\").and.to.be.a(\"string\");\r",
							"    pm.expect(json.booking.bookingdates).to.have.property(\"checkout\").and.to.be.a(\"string\");\r",
							"    pm.expect(json.booking).to.have.property(\"additionalneeds\").and.to.be.a(\"string\");\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
