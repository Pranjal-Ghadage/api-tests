		{
			"name": "update booking",
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"id": "8a7637fc-843b-4649-a053-56e1eb6a6fac",
						"exec": [
							"// 1️⃣ Generate random number for names\r",
							"const randomNum = Math.floor(Math.random() * 9000) + 1000;\r",
							"\r",
							"// 2️⃣ Generate firstname & lastname dynamically\r",
							"const firstName = `user${randomNum}`;\r",
							"const lastName = `test${randomNum}`;\r",
							"\r",
							"pm.environment.set(\"firstName\", firstName);\r",
							"pm.environment.set(\"lastName\", lastName);\r",
							"\r",
							"// Log generated names\r",
							"console.log(\"Generated First Name:\", firstName);\r",
							"console.log(\"Generated Last Name:\", lastName);\r",
							"\r",
							"// 3️⃣ Generate fresh auth token\r",
							"pm.sendRequest({\r",
							"    url: pm.environment.get(\"baseUrl\") + \"/auth\",\r",
							"    method: \"POST\",\r",
							"    header: {\r",
							"        \"Content-Type\": \"application/json\"\r",
							"    },\r",
							"    body: {\r",
							"        mode: \"raw\",\r",
							"        raw: JSON.stringify({\r",
							"            username: \"admin\",\r",
							"            password: \"password123\"\r",
							"        })\r",
							"    }\r",
							"}, function (err, res) {\r",
							"    if (err) {\r",
							"        console.log(\"Error generating auth token:\", err);\r",
							"    } else {\r",
							"        const token = res.json().token;\r",
							"        pm.environment.set(\"auth_token\", token);  // Store fresh token\r",
							"        console.log(\"Generated new auth token:\", token);\r",
							"\r",
							"        // Optionally, add token to the request header dynamically\r",
							"        pm.request.headers.add({\r",
							"            key: \"Cookie\",\r",
							"            value: \"token=\" + token\r",
							"        });\r",
							"    }\r",
							"});\r",
							""
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				},
				{
					"listen": "test",
					"script": {
						"id": "e518a7ad-2ec7-4e80-accb-e740d964abb7",
						"exec": [
							"let json={};\r",
							"try\r",
							"{\r",
							"    json=pm.response.json();\r",
							"}\r",
							"catch(e)\r",
							"{\r",
							" console.log(e);\r",
							"}\r",
							"    \r",
							"pm.test(\"status check\",()=>{\r",
							"    pm.expect(pm.response.code).to.equal(200);\r",
							"});\r",
							"pm.test(\"Content-Type is JSON or text/plain\", () => {\r",
							"    const contentType = pm.response.headers.get(\"content-type\") || \"\";\r",
							"    pm.expect(contentType.toLowerCase()).to.satisfy(type =>\r",
							"        type.includes(\"application/json\") || type.includes(\"text/plain\")\r",
							"    );\r",
							"});\r",
							"pm.test(\"response time\",()=>{\r",
							"    pm.expect(pm.response.responseTime).to.be.below(1000);\r",
							"});\r",
							"pm.test(\" objects\", () => {\r",
							"    \r",
							"    pm.expect(json).to.have.property(\"firstname\").and.to.be.a(\"string\");\r",
							"    pm.expect(json).to.have.property(\"lastname\").and.to.be.a(\"string\");;\r",
							"    pm.expect(json).to.have.property(\"totalprice\").and.to.be.a(\"number\");;\r",
							"    pm.expect(json).to.have.property(\"depositpaid\").and.to.be.a(\"boolean\");\r",
							"    pm.expect(json).to.have.property(\"bookingdates\").and.to.be.an(\"object\");;\r",
							"    pm.expect(json.bookingdates).to.have.property(\"checkin\").and.to.be.a(\"string\");;\r",
							"    pm.expect(json.bookingdates).to.have.property(\"checkout\").and.to.be.a(\"string\");;\r",
							"    pm.expect(json).to.have.property(\"additionalneeds\").and.to.be.a(\"string\");;\r",
							"    \r",
							"});\r",
							""
						],
						"type": "text/javascript",
